@model DoAn_CuaHangLaptop.Models.TaiKhoan

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<div class="container">
    <div class="mt-5">
        <h4 align="center">Chi Tiết Tài Khoản</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.TenDangNhap)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.TenDangNhap)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.MatKhau)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MatKhau)
            </dd>
        </dl>
    </div>
    <div class="mb-3">
        @Html.ActionLink("Cập Nhật", "Edit", new { tendangnhap = Model.TenDangNhap }, new { @class = "btn btn-primary" })
        <br />
        <a asp-action="Index">Trở về trang danh sách</a>
    </div>
</div>